{
  "name": "mist-ui",
  "version": "0.0.0",
  "description": "A Vue3 Component Library",
  "keywords": [
    "mist-ui",
    "ui",
    "component library",
    "vue",
    "vue3"
  ],
  "license": "MIT",
  "author": "aibayanyu",
  "repository": {
    "type": "git",
    "url": "https://github.com/mist-ui/mist-ui"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "unpkg": "dist/mist-ui.umd.js",
  "jsdelivr": "dist/mist-ui.umd.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js"
    },
    "./es": "./es/index.js",
    "./es/*.js": "./es/*.js",
    "./lib": "./lib/index.js",
    "./lib/*.js": "./lib/*.js",
    "./*": "./*"
  },
  "files": [
    "dist",
    "lib",
    "es",
    "volar.d.ts"
  ],
  "sideEffects": [
    "dist/*",
    "es/**/*.css",
    "es/**/*.less",
    "lib/**/*.css",
    "lib/**/*.less",
    "es/*/style/*",
    "lib/*/style/*"
  ],
  "dependencies": {
    "@mist-ui/icons": "^0.0.1",
    "@mistjs/lodash": "^0.0.2",
    "@vueuse/core": "^8.9.4",
    "classnames": "^2.3.2",
    "vue": "^3.2.39"
  },
  "devDependencies": {
    "@types/node": "^18.7.17",
    "@vue/shared": "^3.2.39",
    "typescript": "^4.8.3",
    "unplugin-vue-define-options": "^0.6.2",
    "vite": "^3.1.0",
    "vitest": "^0.18.1"
  },
  "homepage":"https://www.mist-ui.cn"
}
